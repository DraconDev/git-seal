# Example .env file for git-seal demonstration
# This file shows the types of secrets that would be protected

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/myapp
DATABASE_PASSWORD=super_secure_password_123

# API Keys
STRIPE_SECRET_KEY=sk_test_1234567890abcdef
OPENAI_API_KEY=sk-proj-1234567890abcdef
SENDGRID_API_KEY=SG.xxxxxxxxxxxxxxxxxxxxxxxx

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-for-token-signing
JWT_EXPIRES_IN=24h

# External Services
REDIS_URL=redis://localhost:6379/0
AWS_ACCESS_KEY_ID=AKIA1234567890ABCD
AWS_SECRET_ACCESS_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Application Settings
NODE_ENV=development
PORT=3000
SESSION_SECRET=your-session-secret-key